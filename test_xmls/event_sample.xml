<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<events>
    <event name="touchSample" type="touch" xy="30/50" action="DOWN_AND_UP" />

    <event name="keyPressSample" type="press" arg="A" action="DOWN" />

    <event name="dragSample" type="drag" xy="30/50/200/50" duration="0.3" />

    <event name="typeSample" type = "type" arg="abcd" />

    <event name="wakeSample" type="wake" />

    <event name="rebootSample" type="reboot" />

    <event name="getPropertySample" type="getProp" arg="display.width" />

    <event name="shellSample" type="shell" arg="ls" />

    <event name="snapshotSample" type="snapshot" arg="~/snapshot.png" />
</events>
